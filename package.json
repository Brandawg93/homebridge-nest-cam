{
  "displayName": "Homebridge Nest Cam",
  "name": "homebridge-nest-cam",
  "version": "3.4.2",
  "description": "Nest cam plugin for homebridge: https://homebridge.io/",
  "main": "dist/index.js",
  "license": "GPL-3.0",
  "bin": {
    "homebridge-nest-cam": "./cli.sh"
  },
  "scripts": {
    "clean": "rimraf ./dist ./coverage",
    "build": "rimraf ./dist ./coverage && tsc",
    "packlist": "npm pack --dry-run && rm *.tgz",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "jest --coverage",
    "watch:tests": "jest --watch",
    "lint": "eslint **/*.ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "postpublish": "npm run clean"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Brandawg93/homebridge-nest-cam"
  },
  "bugs": {
    "url": "https://github.com/Brandawg93/homebridge-nest-cam/issues"
  },
  "engines": {
    "node": ">=10",
    "homebridge": ">=1.0.0"
  },
  "files": [
    "LICENSE",
    "dist/**/*",
    "images/offline.jpg",
    "config.schema.json",
    "cli.sh"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "ffmpeg-for-homebridge": "^0.0.6",
    "ip": "^1.1.5",
    "pbf": "^3.2.1",
    "get-port": "^5.1.1",
    "execa": "^4.0.3",
    "puppeteer": "^3.3.0",
    "puppeteer-extra": "^3.1.11",
    "puppeteer-extra-plugin-stealth": "2.4.6"
  },
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/jest": "^26.0.0",
    "@types/node": "14.0.19",
    "@types/pbf": "^3.0.2",
    "@types/puppeteer": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "homebridge": "^1.0.4",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.8.3"
  }
}
