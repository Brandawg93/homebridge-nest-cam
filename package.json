{
  "displayName": "Homebridge Nest Cam",
  "name": "homebridge-nest-cam",
  "version": "7.4.0",
  "description": "Nest cam plugin for homebridge: https://homebridge.io/",
  "main": "dist/index.js",
  "license": "GPL-3.0",
  "author": "Brandawg93",
  "funding": [
    {
      "type": "individual",
