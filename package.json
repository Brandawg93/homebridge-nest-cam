    "eslint-plugin-prettier": "^3.4.0",
    "homebridge": "^1.3.4",
    "jest": "^27.0.6",
    "jest-preset-angular": "^9.0.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rxjs": "~7.1.0",
    "ts-jest": "^27.0.3",
    "ts-node": "~10.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  }
