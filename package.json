{
  "displayName": "Homebridge Nest Cam",
  "name": "homebridge-nest-cam",
  "version": "4.7.0",
  "description": "Nest cam plugin for homebridge: https://homebridge.io/",
  "main": "dist/index.js",
  "license": "GPL-3.0",
  "author": {
    "name": "Brandon McFarlin",
    "email": "brandonmcfarlin1016@gmail.com",
    "url": "https://github.com/Brandawg93/"
  },
  "funding": [
    {
      "type": "paypal",
      "url": "https://www.paypal.com/cgi-bin/webscr?cmd=_donations&business=CEYYGVB7ZZ764&item_name=homebridge-nest-cam&currency_code=USD&source=url"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/Brandawg93"
    }
  ],
  "bin": {
    "homebridge-nest-cam": "./cli.sh"
  },
  "scripts": {
    "clean": "rimraf ./dist ./coverage",
    "minify": "minify dist -d dist",
    "build": "rimraf ./dist ./coverage && tsc",
    "packlist": "npm pack --dry-run && rm *.tgz",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "jest --coverage",
    "watch:tests": "jest --watch",
    "lint": "eslint './src/**/*.ts' --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "postpublish": "npm run clean"
  },
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "nest",
    "nest-cam",
    "doorbell",
    "camera",
    "smart"
  ],
  "repository": "github:Brandawg93/homebridge-nest-cam",
  "bugs": {
    "url": "https://github.com/Brandawg93/homebridge-nest-cam/issues"
  },
  "engines": {
    "node": ">=10",
    "homebridge": ">=1.0.0"
  },
  "files": [
    "LICENSE",
    "dist/**/*",
    "images/offline.jpg",
    "config.schema.json",
    "cli.sh"
  ],
  "dependencies": {
    "systeminformation": "^4.26.10",
    "axios": "^0.19.2",
    "ffmpeg-for-homebridge": "^0.0.7",
    "pbf": "^3.2.1",
    "get-port": "^5.1.1",
    "execa": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "@types/pbf": "^3.0.2",
    "@types/puppeteer": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "homebridge": "^1.1.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6",
    "babel-minify": "^0.5.1"
  },
  "optionalDependencies": {
    "puppeteer": "^5.2.1",
    "puppeteer-extra": "^3.1.12",
    "puppeteer-extra-plugin-stealth": "^2.4.14"
  }
